{
  "name": "wapes-bal-system",
  "version": "1.0.2",
  "description": "WAPES BAL system web application for the WAPES League community",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf public data/*.json data/timestamp.js",
    "build": "yarn clean && yarn build:data && yarn build:frontend && node scripts/set-csp.js",
    "build:frontend": "NODE_ENV=production rollup -c rollup.config.js",
    "build:data": "node scripts/generate-data.js",
    "dev": "rimraf public && rollup -c -w --environment NODE_ENV:development",
    "start": "vercel -A vercel.dev.json dev"
  },
  "author": "Freddy Escobar",
  "license": "MIT",
  "repository": "git@github.com:freddy38510/wapes-bal-system.git",
  "private": true,
  "browserslist": [
    "defaults",
    "safari 11",
    "ie 11"
  ],
  "engines": {
    "node": "^12 || ^14"
  },
  "dependencies": {
    "@googleapis/sheets": "0.3.0",
    "dotenv": "14.1.0",
    "lit": "2.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.16.5",
    "@open-wc/building-rollup": "2.0.1",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-strip": "2.1.0",
    "@web/parse5-utils": "1.3.0",
    "autoprefixer": "10.4.2",
    "cssnano": "5.0.15",
    "deepmerge": "4.2.2",
    "eslint": "8.7.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-lit": "1.6.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-wc": "1.3.2",
    "parse5": "6.0.1",
    "postcss": "8.4.5",
    "postcss-load-config": "3.1.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "rollup": "2.64.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-hoist-import-deps": "^1.1.0",
    "rollup-plugin-inline-svg": "2.0.0",
